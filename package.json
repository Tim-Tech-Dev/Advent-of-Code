{
    "private": true,
    "name": "advent-of-code",
    "version": "2021",
    "description": "My Advent of Code solutions",
    "scripts": {
        "preaddYear": "tsc -b .scripts",
        "addYear": "node .scripts/addYear.js",
        "postaddYear": "npm run format",
        "prebuild": "tsc -b .scripts",
        "build": "node .scripts/build.js",
        "pretest": "tsc -b .scripts",
        "test": "node .scripts/test.js",
        "prestart": "tsc -b .scripts",
        "start": "node .scripts/start.js",
        "format": "prettier --write --ignore-path '**/*.d.ts' .",
        "lint": "eslint --ext ts --ignore-pattern '**/*.d.ts' .",
        "build:2021": "npm run build 2021",
        "test:2021": "npm run test 2021",
        "start:2021": "npm run start 2021",
        "build:all": "npm run build all",
        "test:all": "npm run test all",
        "start:all": "npm run start all"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Tim-Tech-Dev/Advent-of-Code.git"
    },
    "keywords": [
        "Advent of Code"
    ],
    "author": "Tim-Tech-Dev",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Tim-Tech-Dev/Advent-of-Code/issues"
    },
    "homepage": "https://github.com/Tim-Tech-Dev/Advent-of-Code#readme",
    "devDependencies": {
        "@types/chai": "^4.2.22",
        "@types/mocha": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.5.0",
        "@typescript-eslint/parser": "^5.5.0",
        "chai": "^4.3.4",
        "eslint": "^8.3.0",
        "mocha": "^9.1.3",
        "prettier": "^2.5.0"
    },
    "dependencies": {
        "@types/node": "^16.11.11",
        "chalk": "^4.1.2",
        "strip-ansi": "^6.0.1",
        "tslog": "^3.3.0",
        "typescript": "^4.5.2"
    }
}
